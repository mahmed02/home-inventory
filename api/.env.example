NODE_ENV=development
PORT=4000
DATABASE_URL=postgres://postgres:postgres@localhost:5432/home_inventory
ENABLE_DEV_ROUTES=true
REQUIRE_AUTH=false
BASIC_AUTH_USER=
BASIC_AUTH_PASS=
REQUIRE_USER_ACCOUNTS=false
SESSION_TTL_HOURS=720
CORS_ALLOW_ORIGINS=
EMBEDDINGS_PROVIDER=deterministic
EMBEDDING_DIMENSIONS=64

# Public base URL used by clients/shortcuts in deployed environments
APP_BASE_URL=http://localhost:4000

# AWS deployment baseline
AWS_REGION=us-east-1
S3_BUCKET=home-inventory-photos-dev

# Use IAM role in EC2/ECS when possible. If not using role-based auth, set keys:
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
